<table id="tabela_parcelas" class="cell-border responsive-table">
  <thead>
    <tr>
      <th>Número do Processo</th>
      <th>Valor da Parcela</th>
      <th>Valor A.J</th>
      <th>Data Vencimento</th>
      <th>Data Rec. Rda.</th>
      <th>Data Pago Rte.</th>
      <th>Pagar Rte.</th>
      <th>Ações</th>
    </tr>
  </thead>              
  <tbody>   

    <%
    for (var i = 0; i < data.todas_parcelas.length; i++) {          
      %>

      <tr>
        <td><%= data.todas_parcelas[i].numero_processo %></td>
        <td>R$<%= data.todas_parcelas[i].valor %></td>
        <td>R$<%= data.todas_parcelas[i].acessor_juridico %></td>
        <td>
          <span class="hide"><%= data.todas_parcelas[i].data_vencimento_filtro %></span>
          <%= data.todas_parcelas[i].data_vencimento %>
          </td>
        <td>
          <span class="hide"><%= data.todas_parcelas[i].data_recebimento_reclamada_filtro %></span>
          <%= data.todas_parcelas[i].data_recebimento_reclamada %>
          </td>
        <td>
           <span class="hide"><%= data.todas_parcelas[i].data_pago_reclamante_filtro %></span>
          <%= data.todas_parcelas[i].data_pago_reclamante %>
          </td>
        <td>R$<%= data.todas_parcelas[i].valor_pagar_reclamante %></td>
        <td>

          <a data-link="/sistema/relatorios/editar-parcela/<%= data.todas_parcelas[i].id %>" class="load_especifico_to_container pointer" data-idload="<%=i%>" data-container="container_parcelas">
            <i class="fa fa-pencil-square-o bordo-text" aria-hidden="true"></i>
          </a>
          <i class="fa fa-times-circle red-text modal-remover-mount-reload pointer" data-href="#modaldeletar_reload" data-id="<%= data.todas_parcelas[i].id %>" data-texto="Você tem certeza que quer deletar a parcela no valor de R$<%= data.todas_parcelas[i].valor %>? do processo <%= data.todas_parcelas[i].numero_processo %> " data-to="/sistema/processos/desativar-parcela/" data-container="#container_tabela_parcelas_reload" aria-hidden="true"></i>



        </td>
      </tr>

      <%}%>

    </tbody>
  </table>